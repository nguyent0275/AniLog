<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>Display Dynamic Images</h1>
    <div class="row">
      {{#each imageList}}
        <div class="col-md-6">
          <div
            class="text-success"
            style="font-size:large; 
            font-weight:700;"
          >{{this.name}}</div>
          <img src="{{this.src}}" />
        </div>
      {{/each}}
    </div>
  </body>
</html>